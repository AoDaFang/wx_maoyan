
page{
	background-color: #F8F8F8;
}
.arrow{
	width: 0;
	height: 0;
	border: 12rpx solid #b0b0b0;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-right-color: transparent;
}
.one_line{
	white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.top_city{
	max-width: 150rpx;
}
.top{
	display: flex;
	height: 120rpx;
	border-bottom: 1px solid #ddd;
	text-align: center;
	line-height: 120rpx;
	justify-content: space-around;
	position: relative;
}
.select-box{
	display: flex;
	width: 380rpx;
	justify-content: space-between;
}
.select-box-item{
	padding-left: 5px;
}
.select-box::after{
	content: "";
	display: block;
	position: absolute;
	bottom: -1px;
	width: 140rpx;
	height: 2px;
	background-color: #ef4238;
	-webkit-transition:left 0.2s;
	transition: left 0.2s;
}
.coming::after{
	left: 440rpx;
}
.subpage{
	position: absolute;
	width: 100%;
	top: 120rpx;
	bottom: 0;
	overflow: scroll;
}
.subpage::-webkit-scrollbar{
	width: 0;
	height: 0;
	color: transform;
}
.most-list{
	width: 100%;
	height: 400rpx;
	display: flex;
	overflow-x: scroll;
	white-space: nowrap;
}
.most-list::-webkit-scrollbar{
	width: 0;
	height: 0;
	color: transform;
}
.movie-item{
	display: flex;
	box-sizing: border-box;
	height: 226rpx;
	padding-left: 24rpx;
	padding-top: 24rpx;
	padding-bottom: 24rpx;
	border-bottom: 1px solid #ddd;
}
.mostExperted-item{
	margin-left: 10rpx;
	margin-right: 10rpx;
	width: 300rpx;
	height: 400rpx;
	box-sizing: border-box;
	position: relative;
}
.movie_info{
